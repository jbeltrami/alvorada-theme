{
  "name": "alvorada",
  "version": "1.0.0",
  "description": "The \"_s\" for Timber: a dead-simple theme that you can build from. The primary purpose of this theme is to provide a file structure rather than a framework for markup or styles. Configure your Sass, scripts, and task runners however you would like!",
  "main": "",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "twigPrintWidth": 120,
    "twigMultiTags": [
      "nav,endnav",
      "switch,case,default,endswitch",
      "ifchildren,endifchildren",
      "cache,endcache",
      "js,endjs"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.19.6",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^9.0.0",
    "browser-sync": "^2.27.10",
    "cssnano": "^5.1.15",
    "dotenv": "^16.0.3",
    "gulp": "^4.0.2",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "jquery": "^3.6.0",
    "sass": "^1.54.9",
    "prettier-plugin-twig-melody": "^0.4.6",
    "slick-carousel": "^1.8.1",
    "webpack-stream": "^7.0.0"
  },
  "scripts": {
    "start": "gulp watch"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.10.3",
    "gulp-imageoptim": "^1.0.9"
  }
}
